
@{
    ViewData["Title"] = "Association libelle-categories";
    @model AssLibCategorieViewModel
}

<div class="row">
    <div class="col-lg-12">
        <div class="page-header">
            <h1 id="forms">Liste des Association libelle-categories</h1>
        </div>
    </div>
</div>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ListAssLibCategories.FirstOrDefault().Libelle)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ListAssLibCategories) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Libelle)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Modifier</a>
                <a asp-action="Details" asp-route-id="@item.Id">DÃ©tails</a>
            </td>
        </tr>
}
    </tbody>
</table>